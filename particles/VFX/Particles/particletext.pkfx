CCompilerSyntaxNodeExpression	$LOCAL$/CCompilerSyntaxNodeExpression_5566F5E7
{
	Expression = "function void Eval()
{
	Size = SizeControl;
	Color = float4(ColorControl.x, ColorControl.y, ColorControl.z, ColorControl.w);

	float numberOfDigits = 8;
	int particleId = int(spawner.LifeRatio * numberOfDigits - 0.1);
	
	float4 selector = select(float4(0), Digits0, particleId == int4(0,1,2,3))
		+ select(float4(0), Digits1, particleId == int4(4,5,6,7)); 
	TextureID = selector.x + selector.y + selector.z + selector.w; // extract correct digit

	// center text
	float4 digits0NotEmptyMask = float4(Digits0.x != 10, Digits0.y != 10, Digits0.z != 10, Digits0.w != 10);
	float4 digits1NotEmptyMask = float4(Digits1.x != 10, Digits1.y != 10, Digits1.z != 10, Digits1.w != 10);
	float activeDigits = dot(digits0NotEmptyMask, digits0NotEmptyMask) + dot(digits1NotEmptyMask, digits1NotEmptyMask);

	Life = select(2.0, 0.0, (particleId <= 3 && dot(digits0NotEmptyMask, selector) == 0) || (particleId >= 4 && dot(digits1NotEmptyMask, selector) == 0));
	Position = float3(-(particleId - (activeDigits - 1) * 0.5) * Size * DigitsSeparation,0,0);
	Velocity = float3(VelocityControl.x, VelocityControl.y, VelocityControl.z);
}
";
}
